    "react": "16.13.1",
    "react-native": "0.62.1",
    "react-native-fast-image": "8.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@react-native-community/eslint-config": "^1.0.0",
    "@types/jest": "^25.2.1",
    "@types/md5": "^2.1.33",
    "@types/qs": "^6.9.1",
    "@types/ramda": "^0.27.19",
    "@types/react": "^16.9.49",
