  },
  "dependencies": {
    "md5": "^2.3.0",
    "mobx": "^5.15.7",
    "mobx-react": "6.3.0",
    "mobx-state-tree": "^3.17.2",
    "qs": "6.9.4",
    "ramda": "^0.27.1",
    "react": "16.13.1",
    "react-native": "0.64.1",
    "react-native-fast-image": "8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/runtime": "^7.12.18",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.14",
    "@types/md5": "^2.2.0",
    "@types/qs": "^6.9.5",
    "@types/ramda": "^0.27.19",
    "@types/react": "^16.9.49",
    "@types/react-native": "^0.63.23",
    "@types/react-test-renderer": "^16.9.3",
    "@types/uuid": "^8.3.0",
    "babel-jest": "^26.3.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "6.12.0",
